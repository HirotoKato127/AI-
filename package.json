{
  "name": "ai-",
  "version": "1.0.0",
  "description": "企業の採用活動とKPI管理を統合したWebダッシュボードアプリケーション。シンプルで読みやすいコードアーキテクチャを採用し、外部依存を最小限に抑えた設計となっています。",
  "main": "dashboard.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "sync:kintone": "node scripts/syncKintone.js",
    "backup:db": "bash scripts/backup/db_backup.sh",
    "restore:db": "bash scripts/backup/db_restore.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "npx tailwindcss -i ./input.css -o ./output.css --minify",
    "predeploy:cloudflare": "npm run build:css",
    "predeploy:cloudflare:prod": "npm run build:css",
    "deploy:cloudflare": "npx wrangler pages deploy . --project-name agent-key --commit-dirty=true",
    "deploy:cloudflare:prod": "npx wrangler pages deploy . --project-name agent-key --commit-dirty=true --branch=main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teraikosuke/AI-.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teraikosuke/AI-/issues"
  },
  "homepage": "https://github.com/teraikosuke/AI-#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "pg": "^8.18.0",
    "puppeteer": "^24.36.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
